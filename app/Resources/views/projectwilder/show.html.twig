{% extends 'base.html.twig' %}

{% block body %}
    <h1>Projectwilder</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ projectWilder.id }}</td>
            </tr>
            <tr>
                <th>Visibility</th>
                <td>{% if projectWilder.visibility %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('projectwilder_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('projectwilder_edit', { 'id': projectWilder.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
